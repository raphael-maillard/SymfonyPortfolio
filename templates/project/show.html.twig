{% extends 'base.html.twig' %}

{% block title %}Project{% endblock %}

{% block body %}
    <h1 class="mt-5">Project</h1>

    <table class="table">
        <tbody>
            <tr>
                <th>Id</th>
                <td>{{ project.id }}</td>
            </tr>
            <tr>
                <th>Name</th>
                <td>{{ project.name }}</td>
            </tr>
            <tr>
                <th>ImageName</th>
                <td>{{ project.imageName }}</td>
            </tr>
            <tr>
                <th>Link</th>
                <td>{{ project.link }}</td>
            </tr>
            <tr>
                <th>Resume</th>
                <td>{{ project.resume }}</td>
            </tr>
            <tr>
                <th>LinkSourceCode</th>
                <td>{{ project.linkSourceCode }}</td>
            </tr>
        </tbody>
    </table>

    <a href="{{ path('project_index') }}">back to list</a>

    <a href="{{ path('project_edit', {'id': project.id}) }}" class="btn btn-success">edit</a>

    {{ include('project/_delete_form.html.twig') }}
{% endblock %}
